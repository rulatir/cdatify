{
    "name": "rulatir/cdatify",
    "description": "Wrap XML text nodes in CDATA after decoding entities",
    "type": "project",
    "repositories": {
        "rulatir-xliff-parser": {
            "type": "vcs",
            "url": "https://github.com/rulatir/xliff-parser.git"
        }
    },
    "autoload": {
        "psr-4": {
            "Rulatir\\Cdatify\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Szczepan Ho≈Çyszewski",
            "email": "rulatir@wp.pl"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Rulatir\\Cdatify\\": "tests/"
        }
    },
    "require": {
        "mschop/pathogen": "^0.7.1",
        "ext-dom": "*",
        "thunderer/shortcode": "^0.7.5",
        "league/flysystem": "^3.12",
        "vanilla/garden-cli": "^3.1",
        "ext-readline": "*",
        "adhocore/cli": "^1.2",
        "cocur/slugify": "^4.5",
        "scotteh/php-dom-wrapper": "^3.0",
        "matecat/xliff-parser": "dev-master",
        "ext-libxml": "*",
        "gravitypdf/querypath": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "ext-xdebug": "*"
    }
}
